{
    "name": "molajo/ioc",
    "type": "molajo-package",
    "description": "Molajo Inversion of Control package offers a full-featured, simple-to-use dependency injection and object construction solution for PHP applications. ",
    "keywords": [
        "IoC",
        "Molajo",
        "Inversion of Control",
        "Dependency Injection"
    ],
    "homepage": "http://github.com/Molajo/IoC",
    "license": "MIT",
    "authors": [
        {
            "name": "Amy Stephen",
            "email": "AmyStephen@Molajo.org",
            "homepage": "http://AmyStephen.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "commonapi/event": "dev-master",
        "commonapi/ioc": "dev-master",
        "commonapi/query": "dev-master",
        "commonapi/resource": "dev-master"
    },
    "support": {
        "email": "hello@molajo.org",
        "irc": "irc://irc.freenode.org/molajo"
    },
    "autoload": {
        "psr-4": {
            "Molajo\\IoC\\": "Source/"
        }
    },

    "archive": {
        "exclude": ["/.dev"]
    },

    "minimum-stability": "dev"
}
